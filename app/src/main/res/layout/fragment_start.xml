<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:background="@color/colorPrimary">

    <ImageView
            android:id="@+id/fuckleLogo"
            android:layout_width="0dp"
            android:layout_height="@dimen/fragment_start_logo_height"
            android:tint="@color/fragment_start_logo_tint"
            android:src="@drawable/ic_fuckle_logo"
            android:layout_marginTop="@dimen/fragment_start_margin_logo"
            android:layout_marginBottom="@dimen/fragment_start_margin_logo"
            android:layout_marginStart="@dimen/fragment_start_margin_logo"
            android:layout_marginEnd="@dimen/fragment_start_margin_logo"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

</android.support.constraint.ConstraintLayout>